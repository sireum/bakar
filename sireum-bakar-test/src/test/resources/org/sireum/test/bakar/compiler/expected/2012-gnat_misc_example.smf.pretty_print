package Example;


procedure add (
        Integer x,
        Integer y) {

  #l1.
    x := x + y;

  #l2.
    return;
}

procedure foo (
        Integer A,
        Integer B) {
  local
    Integer M;


  #l3.
    M := 3ii;

  #l4.
    call add (A, M);

  #l5.
    call add (B, 5ii);

  #l6.
    A := A + 2ii;

  #l7.
    B := B + 5ii;

  #l8.
    return;
}

